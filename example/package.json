{
  "name": "react-native-nitro-screen-brightness-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preandroid": "rm -rf ../android/build",
    "android": "react-native run-android -- --extra-params \"--init-script android/init.gradle\"",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "prebuild:android": "rm -rf ../android/build",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a --init-script android/init.gradle\"",
    "build:ios": "react-native build-ios --mode Debug",
    "pod-install": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-community/slider": "^5.1.1",
    "react": "19.2.0",
    "react-native": "0.83.0",
    "react-native-nitro-modules": "^0.32.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.83.0",
    "@react-native/metro-config": "0.83.0",
    "@react-native/typescript-config": "0.83.0",
    "@types/react": "^19.2.0",
    "react-native-builder-bob": "^0.40.17",
    "react-native-monorepo-config": "^0.3.1"
  },
  "engines": {
    "node": ">=20"
  }
}
